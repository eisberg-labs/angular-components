{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-size-me": {
      "projectType": "library",
      "root": "projects/ngx-size-me",
      "sourceRoot": "projects/ngx-size-me/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngx-size-me/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-size-me/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-size-me/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-size-me/src/test.ts",
            "tsConfig": "projects/ngx-size-me/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-size-me/karma.conf.js",
            "codeCoverage": true
          }
        }
      }
    },
